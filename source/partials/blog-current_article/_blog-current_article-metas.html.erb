<ul class="meta">
  <li class="date"><i class="fa fa-clock-o"></i> <span class="posted-date">Posted on <%= current_article.date.strftime('%b %e, %Y') %></span></li>
  <i class="fa fa-tags"></i> <% current_article.tags.each do |tag| %>
  <li class="tags tag-label"><%= link_to tag, tag_path(tag) %></li>
  <% end %>
</ul>
